{
  "name": "siteit",
  "version": "0.9.8",
  "description": "CLI tool that converts user entered data via terminal into static website",
  "main": "./build/index.js",
  "type": "module",
  "bin": {
    "siteit": "./build/index.js"
  },
  "scripts": {
    "lint": "npx eslint .",
    "lint:fix": "npm run lint -- --fix",
    "prettier:check": "npx prettier --check .",
    "prettier:format": "npx prettier --write .",
    "pre-commit": "npx lint-staged && pretty-quick --staged",
    "build": "babel ./src -d build",
    "start": "npm run build && node ./build/index.js -v",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "prepare": "husky install"
  },
  "keywords": [
    "SSG"
  ],
  "author": "Taimoor Dawami",
  "license": "MIT",
  "dependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "chalk": "^4.1.2",
    "highlight.js": "^11.6.0",
    "pretty": "^2.0.0",
    "remarkable": "^2.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/node": "^7.19.1",
    "@babel/plugin-proposal-throw-expressions": "^7.18.6",
    "@babel/plugin-syntax-import-assertions": "^7.18.6",
    "@babel/preset-env": "^7.19.4",
    "@jest/globals": "^29.3.1",
    "babel-plugin-import": "^1.13.5",
    "eslint": "^8.27.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.0",
    "jest": "^29.3.1",
    "jest-esm-transformer": "^1.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "2.8.0",
    "pretty-quick": "^3.1.3"
  }
}
